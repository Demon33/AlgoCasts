//array = [1, 2, 3, 4, 5, 6] --> s : 21 p :  720


function charSum(array){
let sum = 0;
let i = array.length;

while(i--)
    sum += array[i];
return sum;
}

//or

function charSum(array){
let s = 0;
let p = 1;

for (i = 0; i < array.length; i ++) 
   {
    s += array[i]; //sum
    p *= array[i]; //product
    
    }
return s;
}
